{
    "welcomeFile": "capapplication/webapp/",
    "authenticationMethod": "route",
    "sessionTimeout": 100,
    "routes": [
        {
            "source": "^/capapplication/webapp/(.*)",
            "target": "$1",
            "localDir": "capapplication/webapp/",
            "authenticationType": "xsuaa"
        },
        {
            "source": "^/(.*)$",
            "destination": "srv-api",
            "authenticationType": "xsuaa"
        }
    ]
}